        "\n",
        "# --------------- First world: Fighting game charachter definitions ---------------\n",
        "# Users characteristics for fighting game\n",
        "you = You(name=input(\"Please enter your name to start the game: \"), health=100)\n",
        "# Charachteristics of twin brother for fighting game\n",
        "twinbrother = TwinBrother(name=\"Man with eyepatch\", health=100, weapon=sword)\n",
        "\n",
        "# --------------- Movieportal game ---------------\n",
        "class movieportal_game():\n",
        "    def __init__(self, name:str, points: int):\n",
        "        # Game is not over\n",
        "        self.game_over = False\n",
        "        # Defining self.points\n",
        "        self.points = points\n",
        "\n",
        "        # User starts with no portal pieces (empty list)\n",
