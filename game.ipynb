        "        print(\"You grab her braid and look down. It is definitely a pretty high tower. The wind whips your face. You are scared.\")\n",
        "        sleep(1)\n",
        "        # List of possible answers\n",
        "        answers = [\"y\", \"n\"]\n",
        "        # Pre-define answer user will put in\n",
        "        answer = \"\"\n",
